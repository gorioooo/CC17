<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgs"
    tools:context=".VicenteFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="16dp"
        android:paddingTop="16dp"
        android:paddingRight="16dp"
        android:paddingBottom="16dp"
        tools:context=".MainActivity">

        <Button
            android:id="@+id/vicenteButton"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="126dp"
            android:layout_marginEnd="121dp"
            android:layout_marginBottom="106dp"
            android:clickable="true"
            android:focusable="true"
            android:onClick="https://www.catholic.org/saints/saint.php?saint_id=1996"
            android:text="@string/open_link"
            android:textColor="#263238"
            tools:ignore="OnClick,UsingOnClickInXml,VisualLintButtonSize" />

    </RelativeLayout>
</RelativeLayout>